{
    "name": "San Fransisco Dark",
    // "editor.fontFamily": "Menlo, Monaco, 'Courier New', monospace",
    // "editor.fontFamily": "SF Mono",
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook"
    },
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": "keyword.control",
                "settings": {
                    "fontStyle": "italic bold",
                    "foreground": "#c0d0ed",
                }
            },
            {
                "scope": "variable.language",
                "settings": {
                    "foreground": "#a29ef1",
                }
            },
            {
                "scope": "string",
                "settings": {
                    // "fontStyle": "italic",
                    "foreground": "#bae1bc",
                }
            },
            {
                "scope": "entity.name.type.class",
                "settings": {
                    "foreground": "#7391e4",
                    "fontStyle": "bold"
                }
            },
            {
                "scope": "comment",
                "settings": {
                    "foreground": "#d1efe3",
                    "fontStyle": "italic",
                }
            },
            {
                "scope": "storage.type",
                "settings": {
                    "foreground": "#ec84c1",
                    "fontStyle": "bold"
                }
            },
            {
                "scope": "entity.name.type.module.js",
                "settings": {
                    "foreground": "#9e80ff",
                    "fontStyle": "bold"
                }
            },
            {
                "scope": "entity.name.namespace",
                "settings": {
                    "foreground": "#cdb87d",
                    "fontStyle": "bold"
                }
            },
            {
                "scope": "constant.numeric",
                "settings": {
                    "foreground": "#9eb3f1",
                    "fontStyle": "italic",
                }
            },
            {
                "scope": "support.function.magic.python",
                "settings": {
                    "foreground": "#bae1bc",
                    "fontStyle": "italic",
                }
            },
            {
                "scope": "entity.name.function.member",
                "settings": {
                    "foreground": "#eccc9d",
                }
            },
            {
                "scope": "variable.parameter.function.language.special.self.python",
                "settings": {
                    "foreground": "#94a9e1",
                    "fontStyle": "italic",
                }
            },
            {
                "scope": "keyword.operator.logical.python",
                "settings": {
                    "foreground": "#A167E6",
                }
            },
            {
                "scope": "constant.language.python",
                "settings": {
                    "foreground": "#ec84c1",
                    "fontStyle": "bold",
                }
            },
            {
                "scope": "variable.other.readwrite",
                "settings": {
                    "foreground": "#eccc9d",
                }
            },
            {
                "scope": "entity.name.function",
                "settings": {
                    "foreground": "#ec84c1",
                    "fontStyle": "bold"
                }
            },
            {
                "scope": "entity.name.function.member",
                "settings": {
                    "foreground": "#D0A8FF",
                }
            },
            {
                "scope": "punctuation.separator.parameters.python",
                "settings": {
                    "foreground": "#a29ef1",
                }
            },
        ]
    },
    "workbench.colorCustomizations": {
        "editorLineNumber.activeForeground": "#dcdcff",
        "editorLineNumber.foreground": "#9a9aaf",
        "breadcrumb.foreground": "#ebebeb",
        "activityBar.foreground": "#ebebeb",
        "activityBarBadge.background": "#141414",
        "activityBar.activeBackground": "#141414",
        "activityBar.background": "#1a1a1a",
        "activityBar.activeBorder": "#ff3131",
        "activityBar.border": "#141414",
        "activityBar.inactiveForeground": "#ebebeb",
        "tab.activeBorderTop": "#141414",
        "tab.hoverBackground": "#141414",
        "tab.hoverForeground": "#ff3131",
        "tab.activeBackground": "#141414",
        "toolbar.hoverOutline": "#9b9b9b",
        "debugToolBar.background": "#333333",
        "tab.activeForeground": "#ebebeb",
        "tab.inactiveForeground": "#ebebeb",
        "tab.inactiveBackground": "#141414",
        "sideBar.background": "#1a1a1a",
        "sideBar.border": "#141414",
        "sideBar.foreground": "#ebebeb",
        "sideBarSectionHeader.border": "#141414",
        "sideBarTitle.foreground": "#ebebeb",
        "scrollbarSlider.background": "#000000",
        "scrollbar.shadow": "#31d9ff",
        "scrollbarSlider.hoverBackground": "#141414",
        "sideBarSectionHeader.foreground": "#ebebeb",
        "sideBarSectionHeader.background": "#141414",
        "terminal.background": "#141414",
        "terminal.border": "#141414",
        "terminal.foreground": "#ebebeb",
        "terminalCursor.foreground": "#ff3131",
        "terminal.tab.activeBorder": "#ff3131",
        "input.background": "#141414",
        "input.placeholderForeground": "#ebebeb",
        "list.activeSelectionBackground": "#141414",
        "list.activeSelectionIconForeground": "#ff3131",
        "list.hoverBackground": "#141414",
        "list.hoverForeground": "#ff3131",
        "list.focusOutline": "#141414",
        "list.inactiveFocusOutline": "#141414",
        "list.inactiveSelectionBackground": "#141414",
        "welcomePage.background": "#141414",
        "welcomePage.tileHoverBackground": "#141414",
        "welcomePage.progress.background": "#ebebeb",
        "icon.foreground": "#ebebeb",
        "minimap.background": "#141414",
        "minimap.errorHighlight": "#ff3131",
        "minimap.selectionHighlight": "#ebebeb",
        "editorCursor.foreground": "#ff3131",
        "editorCursor.background": "#000000",
        "statusBar.background": "#141414",
        "badge.background": "#333333",
        "editor.hoverHighlightBackground": "#333333",
        // -------------------------------------------LINES
        "editor.lineHighlightBorder": "#4e4e4e",
        "editor.lineHighlightBackground": "#202020",
        "editor.selectionBackground": "#4e4e4e",
        "editorBracketHighlight.foreground1": "#ffffff",
        "editorBracketHighlight.foreground2": "#ec84c1",
        // "editorBracketHighlight.unexpectedBracket.foreground": "#ff0000",
    },
    "editor.cursorStyle": "line",
    // "breadcrumbs.enabled": false,
    "editor.cursorWidth": 2,
    "editor.cursorBlinking": "phase",
    "editor.formatOnSave": true,
    "editor.hover.delay": 300,
    "editor.minimap.enabled": false,
    "editor.mouseWheelScrollSensitivity": 0.7,
    "editor.smoothScrolling": true,
    "editor.lineDecorationsWidth": 20,
    "editor.wordWrapColumn": 90,
    "editor.lineNumbers": "interval",
    "editor.lineHeight": 2.0,
    "editor.renderLineHighlight": "all",
    "editor.scrollbar.vertical": "visible",
    "editor.scrollbar.verticalScrollbarSize": 0,
    // "editor.glyphMargin": false,
    // "workbench.editor.showTabs": false,
    // line decoration should be 30
    "editor.rulers": [
        0,
        90
    ],
    // used for fonts
    "editor.fontLigatures": true,
    "editor.fontSize": 12.5,
    //------------------------------------- LINES----------------
    "workbench.iconTheme": "material-icon-theme",
    // terminal font
    "terminal.integrated.fontFamily": "SF Mono",
    "editor.wordWrap": "wordWrapColumn",
    "editor.wrappingIndent": "same",
    "editor.guides.indentation": false,
    "zenMode.centerLayout": false,
    "files.associations": {
        "*.ejs": "html",
        "*.rmd": "markdown"
    },
    "editor.stickyScroll.enabled": true,
    "tabnine.experimentalAutoImports": true,
    "workbench.activityBar.visible": false,
    "window.zoomLevel": 1,
    // "workbench.statusBar.visible": false,
}